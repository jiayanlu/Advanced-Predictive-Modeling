   "source": [
    "#(a)\n",
    "df[['rent','inst','ecks','demo_score']].plot(kind = 'box')\n",
    "print 'The cutoff values for outliers is <1.5IQR below 1stQ or >1.5IQR above 3rdQ.'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def f(x):\n",
    "    IQR = x.quantile(0.75)-x.quantile(0.25)\n",
    "    Max = x.quantile(0.75) + 1.5*IQR  \n",
    "    Min = x.quantile(0.25) - 1.5*IQR\n",
    "    return Max, Min\n",
    "rent = f(df['rent'])\n",
    "inst = f(df['inst'])\n",
    "ecks = f(df['ecks'])\n",
    "demo_score = f(df['demo_score'])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "mask_rent = (df['rent'] > rent[0]) | (df['rent'] < rent[1])\n",
    "mask_inst = (df['inst'] > inst[0]) | (df['inst'] < inst[1])\n",
    "mask_ecks = (df['ecks'] > ecks[0]) | (df['ecks'] < ecks[1])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
